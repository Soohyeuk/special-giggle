{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "description": "Description of your extension",
    "permissions": ["tabs"],
    "content_scripts": [{
      "matches": ["https://example.com/*"],
      "js": ["content_script.js"]
    }],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }